		  xmlns:s="library://ns.adobe.com/flex/spark"
		  visible="{ProfileViewModel.IsCheckedIn}" clipAndEnableScrolling="true">
	<fx:Script>
		<![CDATA[
			import viewmodel.base.ProfileViewModel;
		]]>
	</fx:Script>
	<s:Label text="{ProfileViewModel.FirstName}" styleName="profileLabel" />
